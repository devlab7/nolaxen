Sub addElementToList()
Dim list() As String

i = 1
numberOfItems = 0
While Len(Cells(i, 1).Value) > 0
    If Right(Cells(i, 1).Value, 6) = "(arr.)" Or Right(Cells(i, 1).Value, 6) = "(d√©p.)" Then
        ReDim Preserve list(numberOfItems)

        list(numberOfItems) = Cells(i, 1).Value
        numberOfItems = numberOfItems + 1
        
    End If
    i = i + 1
Wend

For Each Item In list
    MsgBox (Item)
Next Item


End Sub
